<html>
    <head>
        <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
        <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
        <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
        <!------ Include the above in your HEAD tag ---------->
    </head>
    <body>
        <div class="container">
            <div class="row">
                <div class="col-xs-12">
                    <div class="invoice-title">
                        <h2><%= client %></h2><br><br>
                    </div>
                    <hr>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><strong>Menu</strong></h3>
                        </div>
                        <div class="panel-body">
                        <form method="post">
                            <div class="table-responsive">
                                <table class="table table-condensed">
                                    <thead>
                                        <tr>
                                            <td><strong>Item</strong></td>
                                            <td class="text-center"><strong>Price</strong></td>
                                            <td class="text-center"><strong>Quantity</strong></td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for (var i = 0; i<menu.length; i++) { %>
                                        <tr>
                                            <td><%= menu[i].foodName%></td>
                                            <td class="text-center"><%= menu[i].price %></td>
                                            <td class="text-center"><%= menu[i].qty %></td>
                                            <td class="text-center"> <button disabled>Deactivate</button> </td>
                                        </tr>
                                        <% } %>
                                        <br>
                                        <tr>
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td class="text-center"> <button type="submit" formaction="clienthome">Go back</button> </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>